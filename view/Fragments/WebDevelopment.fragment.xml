<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:uxap="sap.uxap">
	<uxap:ObjectPageLayout id="WebDevelopmentPageLayout">
		<uxap:headerTitle>
			<uxap:ObjectPageHeader objectTitle="{i18n>webDevelopmentTitle}" objectSubtitle="{i18n>webDevelopmentSubtitle}"/>
		</uxap:headerTitle>
		<uxap:sections>
			<uxap:ObjectPageSection title="{i18n>httpSection}">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>whyHttpSubSection}">
						<uxap:blocks>
							<Text text="{i18n>whyHTTPText}"/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
					<uxap:ObjectPageSubSection title="{i18n>onlineLearningResources}">
						<uxap:blocks>
							<List id="httpLearningList" items="{development>/resources/http}">
								<items>
									<CustomListItem>
										<HBox>
										<!-- <core:Icon src="{development>icon}" class="sapUiSmallMarginEnd"/> -->
										<VBox>
											<Link text="{development>title}" href="{development>uri}" target="_blank" />
											<!-- info="{development>readingStatus}" -->
											<Text text="{development>description}"/>
										</VBox>
									</HBox>
									</CustomListItem>
								</items>
							</List>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>

			<uxap:ObjectPageSection title="{i18n>xmlSection}">
