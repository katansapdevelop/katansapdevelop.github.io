<mvc:View displayBlock="true" 
		  xmlns:mvc="sap.ui.core.mvc" 
		  xmlns="sap.m" 
		  controllerName="com.katan.controller.Root"
		  xmlns:core="sap.ui.core" 
		  xmlns:l="sap.ui.layout" 
		  xmlns:f="sap.ui.layout.form"
		  xmlns:controls="com.katan.control">
	<App>
		<pages>
			<Page title="{i18n>siteTitle}">
				<content>
					<IconTabBar id="idTopLevelIconTabBar" selectedKey="aboutme" class="sapUiResponsiveContentPadding">
						<items>
							<IconTabFilter id="aboutMeTab" text="{i18n>aboutMeTab}" key="aboutme">
								<Text text="{/AboutMe}"/>
								<!-- <controls:Chart id="testChart" chartType="polarArea" width="500px" height="250px" initChart="setChartData"/> -->
							</IconTabFilter>
							<IconTabFilter id="skillsTab" text="{i18n>skillsTab}" key="skills">
								<Carousel>
									<pages>
										<Panel id="programmingLanguagePanel" width="auto" class="sapUiResponsiveMargin">
											<Table id="programmingLanguageTable" items="{/Skills/ProgrammingLanguages}">
												<headerToolbar>
													<Toolbar>
														<Title text="{i18n>programmingLanguagePanel}"/>
													</Toolbar>
												</headerToolbar>
												<columns>
													<Column>
														<Text text="{i18n>programmingLanguageColumnLanguage}"/>
													</Column>
													<Column>
														<Text text="{i18n>programmingLanguageColumnRating}"/>
													</Column>
												</columns>
												<items>
													<ColumnListItem>
														<cells>
															<Label text="{Language}" class="sapUISmallMarginTop"/> 
															<RatingIndicator maxValue="{MaxRating}" value="{Rating}" enabled="false"/>
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</Panel>
										<Panel id="sapAppsPanel" width="auto" class="sapUiResponsiveMargin">
											<Table id="sapApplicationsTable" items="{/Skills/SAPApplications}">
												<headerToolbar>
													<Toolbar>
														<Title text="{i18n>sapAppsPanel}"/>
													</Toolbar>
												</headerToolbar>
												<columns>
													<Column>
														<Text text="{i18n>sapAppColumnApplication}"/>
													</Column>
													<Column>
														<Text text="{i18n>sapAppColumnRating}"/>
													</Column>
												</columns>
												<items>
													<ColumnListItem>
														<cells>
															<Label text="{Application}" class="sapUISmallMarginTop"/> 
															<RatingIndicator maxValue="{MaxRating}" value="{Rating}" enabled="false"/>
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</Panel>
									</pages>
								</Carousel>
							</IconTabFilter>
							<IconTabFilter id="experienceTab" text="{i18n>experienceTab}" key="experience">
							</IconTabFilter>
							<IconTabFilter id="qualificationsTab" text="{i18n>qualificationsTab}" key="qualifications">
								<Carousel>
									<pages>
										<Panel id="certificationsPanel" width="auto" class="sapUiResponsiveMargin">
											<Table id="certificationsTable" items="{/Certifications}">
												<headerToolbar>
													<Toolbar>
														<Title text="{i18n>certificationsPanel}"/>
													</Toolbar>
												</headerToolbar>
												<columns>
													<Column>
														<Text text="{i18n>certificationColumnName}"/>
													</Column>
													<Column>
														<Text text="{i18n>certificationColumnAgency}"/>
													</Column>
													<Column>
														<Text text="{i18n>certificationColumnScore}"/>
													</Column>
												</columns>
												<items>
													<ColumnListItem>
														<cells>
															<Label text="{Name}" class="sapUISmallMarginTop"/> 
															<Label text="{CertifyingAgency}" class="sapUISmallMarginTop"/> 
															<controls:Chart id="certificationChart" chartType="pie" width="500px" height="250px" 
																initChart="setCertificateChartData"/>
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</Panel>
									</pages>
								</Carousel>
							</IconTabFilter>
							<IconTabFilter id="contactTab" text="{i18n>contactTab}" key="contact">
								<IconTabBar id="contactTabBar" selectedKey="email">
									<items>
										<IconTabFilter id="callTab" icon="sap-icon://call" key="call"/>
										<IconTabFilter id="emailTab" icon="sap-icon://email" key="email"/>
										<IconTabFilter id="downloadTab" icon="sap-icon://download" key="download"/>
									</items>
								</IconTabBar>
							</IconTabFilter>
						</items>
					</IconTabBar>
				</content>
			</Page>
		</pages>
	</App>	  
</mvc:View>
