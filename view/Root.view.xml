<mvc:View displayBlock="true" 
		  xmlns:mvc="sap.ui.core.mvc" 
		  xmlns="sap.m" 
		  controllerName="com.katan.controller.Root"
		  xmlns:core="sap.ui.core" 
		  xmlns:l="sap.ui.layout" 
		  xmlns:f="sap.ui.layout.form">

	<App>
		<pages>
			<Page title="Katan's Site">
				<content>
					<IconTabBar id="idTopLevelIconTabBar" selectedKey="db" class="sapUiResponsiveContentPadding">
						<IconTabFilter id="start" text="Tab Title">
							<Carousel>
									<pages>
										<Image src="https://upload.wikimedia.org/wikipedia/commons/9/9f/GEO_Globe.jpg" height="600px"/>
										<Image
											src="https://upload.wikimedia.org/wikipedia/commons/thumb/8/86/Wikipedia_Hello_World_Graphic.svg/2000px-Wikipedia_Hello_World_Graphic.svg.png"
											height="600px"/>
									</pages>
								</Carousel>
						</IconTabFilter>
					</IconTabBar>
				</content>
			</Page>
		</pages>
	</App>	  
</mvc:View>
