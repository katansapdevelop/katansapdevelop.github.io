<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:uxap="sap.uxap">
	<uxap:ObjectPageLayout id="SapDevelopmentPageLayout">
		 <uxap:headerTitle>
			<uxap:ObjectPageHeader objectTitle="{i18n>sapDevelopmentTitle}" objectSubtitle="{i18n>sapDevelopmentSubtitle}"/>
		</uxap:headerTitle>
		<uxap:sections>
			<uxap:ObjectPageSection title="{i18n>sapDesignSection}">
				<uxap:subSections>
					<uxap:ObjectPageSubSection title="{i18n>sapDesignSubSection}">
						<uxap:blocks>
							<List id="softwareDesignLearningList" items="{development>/resources/softwareDesign}">
								<items>
									<CustomListItem>
										<HBox>
										<!-- <core:Icon src="{development>icon}" class="sapUiSmallMarginEnd"/> -->
										<VBox>
											<Link text="{development>title}" href="{development>uri}" target="_blank" />
											<!-- info="{development>readingStatus}" -->
											<Text text="{development>description}"/>
										</VBox>
									</HBox>
									</CustomListItem>
								</items>
							</List>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>

			<uxap:ObjectPageSection title="{i18n>abapSection}">
				<uxap:subSections>
					<!--
					<uxap:ObjectPageSubSection title="Why SAP Gateway?">
						<uxap:blocks>
							<Text text="Coming Soon.."/>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>  -->
					<uxap:ObjectPageSubSection title="Learning Resources">
						<uxap:blocks>
							<List id="abapLearningList" items="{development>/resources/abap}">
								<items>
									<CustomListItem>
										<HBox>
										<!-- <core:Icon src="{development>icon}" class="sapUiSmallMarginEnd"/> -->
										<VBox>
											<Link text="{development>title}" href="{development>uri}" target="_blank" />
											<!-- info="{development>readingStatus}" -->
											<Text text="{development>description}"/>
										</VBox>
									</HBox>
									</CustomListItem>
								</items>
							</List>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
					<uxap:ObjectPageSubSection title="Tooling">
						<uxap:blocks>
							<List id="abapToolingList" items="{development>/tooling/abap}">
								<items>
									<CustomListItem>
										<HBox>
										<!-- <core:Icon src="{development>icon}" class="sapUiSmallMarginEnd"/> -->
										<VBox>
											<Link text="{development>title}" href="{development>uri}" target="_blank" />
											<!-- info="{development>readingStatus}" -->
											<Text text="{development>description}"/>
