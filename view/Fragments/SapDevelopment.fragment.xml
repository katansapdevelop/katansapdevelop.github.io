									</HBox>
									</CustomListItem>
								</items>
							</List>
						</uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>


			<uxap:ObjectPageSection title="{i18n>sapui5Section}">
				<uxap:subSections>
					<!--
					<uxap:ObjectPageSubSection title="Why SAPUI5?">
