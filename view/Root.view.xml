<mvc:View displayBlock="true" 
		  xmlns:mvc="sap.ui.core.mvc" 
		  xmlns="sap.m" 
		  controllerName="com.katan.controller.Root"
		  xmlns:core="sap.ui.core" 
		  xmlns:l="sap.ui.layout" 
		  xmlns:f="sap.ui.layout.form"
		  xmlns:controls="com.katan.control">
	<App>
		<pages>
			<Page title="{i18n>siteTitle}">
				<content>
					<IconTabBar id="idTopLevelIconTabBar" selectedKey="aboutme" class="sapUiResponsiveContentPadding">
						<items>
							<IconTabFilter id="aboutMeTab" text="{i18n>aboutMeTab}" key="aboutme">
								<Text text="{/AboutMe}"/>
								<controls:Chart id="myChart" chartType="polarArea" width="600" height="300" data="setData"/>
							</IconTabFilter>
							<IconTabFilter id="skillsTab" text="{i18n>skillsTab}" key="skills">
								<Carousel>
									<pages>
										<Panel id="programmingLanguagePanel" width="auto" class="sapUiResponsiveMargin">
											<List items="{/Skills/ProgrammingLanguages}" headerText="{i18n>programmingLanguagePanel}">
												<CustomListItem>
													<Label text="{Language}" class="sapUISmallMarginTop"/> 
													<RatingIndicator maxValue="{MaxRating}" value="{Rating}" enabled="false"/>
												</CustomListItem>
											</List>
										</Panel>
										<Panel id="sapAppsPanel" width="auto" class="sapUiResponsiveMargin">
											<List items="{/Skills/SAPApplications}" headerText="{i18n>sapAppsPanel}">
												<CustomListItem>
													<Label text="{Application}" class="sapUISmallMarginTop"/> 
													<RatingIndicator maxValue="{MaxRating}" value="{Rating}" enabled="false"/>
												</CustomListItem>
											</List>
										</Panel>
									</pages>
								</Carousel>
							</IconTabFilter>
							<IconTabFilter id="experienceTab" text="{i18n>experienceTab}" key="experience">
							</IconTabFilter>
							<IconTabFilter id="qualificationsTab" text="{i18n>qualificationsTab}" key="qualifications">
							</IconTabFilter>
							<IconTabFilter id="contactTab" text="{i18n>contactTab}" key="contact">
								<IconTabBar id="contactTabBar" selectedKey="email">
									<items>
										<IconTabFilter id="callTab" icon="sap-icon://call" key="call"/>
										<IconTabFilter id="emailTab" icon="sap-icon://email" key="email"/>
										<IconTabFilter id="downloadTab" icon="sap-icon://download" key="download"/>
									</items>
								</IconTabBar>
							</IconTabFilter>
						</items>
					</IconTabBar>
				</content>
			</Page>
		</pages>
	</App>	  
</mvc:View>
